{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "airbnb", "plugin:prettier/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"no-restricted-exports": 0,
		"react-in-jsx-scope": 0,
		"react/react-in-jsx-scope": 0,
		"react/jsx-filename-extension": 0,
		"prefer-default-export": 0
	}
}
